{
  "expo": {
    "name": "Japan Price Tracker",
    "slug": "japan-price-tracker",
    "version": "1.0.0",
    "backgroundColor": "#EAF5FF",
    "primaryColor": "#12243B",
    "orientation": "portrait",
    "scheme": "japan-price-tracker",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "experiments": {
      "typedRoutes": true
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Allow Japan Price Tracker to use your location to compare store distances and suggest the best buying spot."
        }
      ],
      "expo-localization"
    ],
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ejunpark.japanpricetracker"
    },
    "android": {
      "package": "com.ejunpark.japanpricetracker",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBLfI5pf9JRt1fYZffrM374SjSD5he_xOc"
        }
      },
      "permissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION"]
    }
  }
}
