{
  "name": "japan-price-tracker",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "storybook:native": "EXPO_PUBLIC_STORYBOOK_ENABLED=true expo start",
    "storybook:ios": "EXPO_PUBLIC_STORYBOOK_ENABLED=true expo start --ios",
    "storybook:android": "EXPO_PUBLIC_STORYBOOK_ENABLED=true expo start --android",
    "storybook:web": "npm run storybook:build && http-server storybook-static -p 6006 -s",
    "storybook:build": "storybook build -c .storybook -o storybook-static",
    "storybook:test": "npm run storybook:build && concurrently -k -s first \"http-server storybook-static -p 6006 -s\" \"wait-on tcp:6006 && test-storybook --config-dir .storybook --url http://127.0.0.1:6006\"",
    "test": "jest",
    "publish:wiki": "bash scripts/publish-wiki.sh",
    "build:webapp": "node ./scripts/build-webapp.mjs"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@hookform/resolvers": "^5.2.2",
    "@react-native-community/datetimepicker": "^8.2.0",
    "date-fns": "^4.1.0",
    "expo": "^54.0.0",
    "expo-asset": "~12.0.12",
    "expo-constants": "~18.0.13",
    "expo-font": "~14.0.11",
    "expo-linear-gradient": "~15.0.8",
    "expo-localization": "~17.0.8",
    "expo-location": "~19.0.8",
    "expo-router": "~6.0.23",
    "expo-sqlite": "~16.0.10",
    "leaflet": "^1.9.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.71.1",
    "react-native": "0.81.5",
    "react-native-maps": "1.20.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "zod": "^3.23.8",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@expo/ngrok": "^4.1.0",
    "@storybook/addon-ondevice-actions": "^9.1.4",
    "@storybook/addon-ondevice-controls": "^9.1.4",
    "@storybook/react": "^9.1.17",
    "@storybook/react-native": "^9.1.4",
    "@storybook/react-webpack5": "^9.1.17",
    "@storybook/test-runner": "^0.23.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.10",
    "@types/react-test-renderer": "^19.1.0",
    "babel-loader": "^10.0.0",
    "babel-preset-expo": "^54.0.0",
    "concurrently": "^9.2.1",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "jest-expo": "^54.0.0",
    "react-test-renderer": "19.1.0",
    "storybook": "^9.1.17",
    "typescript": "~5.9.2",
    "wait-on": "^9.0.4",
    "workbox-build": "^7.3.0"
  },
  "jest": {
    "preset": "jest-expo",
    "testMatch": [
      "**/__tests__/**/*.test.ts",
      "**/__tests__/**/*.test.tsx"
    ]
  }
}
